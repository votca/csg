include(CMakeFindDependencyMacro)
find_dependency(Eigen3 NO_MODULE)
find_dependency(Boost 1.53.0 COMPONENTS program_options)
find_dependency(VOTCA_TOOLS NO_MODULE)
include("${CMAKE_CURRENT_LIST_DIR}/VOTCA_CSG_Targets.cmake")
add_executable(VOTCA::csg_inverse IMPORTED)
set_property(TARGET VOTCA::csg_inverse PROPERTY IMPORTED_LOCATION "@CMAKE_INSTALL_FULL_BINDIR@/csg_inverse")
set_target_properties(VOTCA::csg_inverse PROPERTIES VOTCA_CSG_DEFAULTS "@CMAKE_INSTALL_FULL_DATADIR@/votca/xml/csg_defaults.xml")
add_executable(VOTCA::csg_call IMPORTED)
set_property(TARGET VOTCA::csg_call PROPERTY IMPORTED_LOCATION "@CMAKE_INSTALL_FULL_BINDIR@/csg_call")
set_target_properties(VOTCA::csg_call PROPERTIES VOTCASHARE "@CMAKE_INSTALL_FULL_DATADIR@/votca")
set_target_properties(VOTCA::csg_call PROPERTIES VOTCA_PATH "@CMAKE_INSTALL_FULL_BINDIR@")
